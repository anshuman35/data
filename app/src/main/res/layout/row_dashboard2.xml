<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="dashboard2RowModel" type="com.bhubaneswar.app.modules.dashboard.data.model.Dashboard2RowModel"/>
    </data>
    <LinearLayout android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:orientation="vertical" style="@style/groupStyle">
        <ImageView android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_23px"
            android:layout_marginEnd="@dimen/_25px"
            android:layout_marginStart="@dimen/_25px" android:layout_width="@dimen/_24px"/>
        <TextView android:gravity="top|center" android:id="@+id/txtHome"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_3px"
            android:layout_width="wrap_content"
            android:text="@{dashboard2RowModel.txtHome}" style="@style/textStylePoppinsbold10_1"/>
    </LinearLayout>
</layout>
