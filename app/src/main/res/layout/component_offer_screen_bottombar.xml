<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="offerScreenBottombarModel" type="com.bhubaneswar.app.modules.offerscreenbottombar.data.model.OfferScreenBottombarModel"/>
    </data>
    <LinearLayout android:layout_gravity="center_horizontal"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_1">
        <View android:background="@color/blue_50"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1px" android:layout_width="match_parent"/>
        <LinearLayout android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_15px"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStyle">
            <LinearLayout android:layout_gravity="center_vertical"
                android:layout_height="wrap_content"
                android:layout_weight="0.2" android:layout_width="0dp"
                android:orientation="vertical" style="@style/groupStyle">
                <ImageView android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_24px"
                    android:layout_marginEnd="@dimen/_25px"
                    android:layout_marginStart="@dimen/_25px" android:layout_width="@dimen/_24px"/>
                <TextView android:gravity="top|center"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_4px"
                    android:layout_width="match_parent"
                    android:text="@string/lbl_home" style="@style/textStylePoppinsregular10"/>
            </LinearLayout>
            <LinearLayout android:layout_gravity="center_vertical"
                android:layout_height="wrap_content"
                android:layout_weight="0.2" android:layout_width="0dp"
                android:orientation="vertical" style="@style/groupStyle">
                <ImageView android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_24px"
                    android:layout_marginEnd="@dimen/_25px"
                    android:layout_marginStart="@dimen/_25px" android:layout_width="@dimen/_24px"/>
                <TextView android:gravity="top|center"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_4px"
                    android:layout_width="match_parent"
                    android:text="@string/lbl_explore" style="@style/textStylePoppinsregular10"/>
            </LinearLayout>
            <LinearLayout android:layout_gravity="center_vertical"
                android:layout_height="wrap_content"
                android:layout_weight="0.2" android:layout_width="0dp"
                android:orientation="vertical" style="@style/groupStyle">
                <FrameLayout android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_15px"
                    android:layout_marginStart="@dimen/_15px" android:layout_width="match_parent">
                    <ImageView android:layout_gravity="top|end"
                        android:layout_height="@dimen/_20px"
                        android:layout_marginBottom="@dimen/_14px" android:layout_width="@dimen/_20px"/>
                    <ImageView android:layout_gravity="center"
                        android:layout_height="@dimen/_24px" android:layout_width="@dimen/_24px"/>
                </FrameLayout>
                <TextView android:gravity="top|center"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_4px"
                    android:layout_width="match_parent"
                    android:text="@string/lbl_cart" style="@style/textStylePoppinsregular10"/>
            </LinearLayout>
            <LinearLayout android:layout_gravity="center_vertical"
                android:layout_height="wrap_content"
                android:layout_weight="0.2" android:layout_width="0dp"
                android:orientation="vertical" style="@style/groupStyle">
                <ImageView android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_24px"
                    android:layout_marginEnd="@dimen/_25px"
                    android:layout_marginStart="@dimen/_25px" android:layout_width="@dimen/_24px"/>
                <TextView android:gravity="top|center"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_4px"
                    android:layout_width="match_parent"
                    android:text="@string/lbl_offer" style="@style/textStylePoppinsbold10_1"/>
            </LinearLayout>
            <LinearLayout android:layout_gravity="center_vertical"
                android:layout_height="wrap_content"
                android:layout_weight="0.2" android:layout_width="0dp"
                android:orientation="vertical" style="@style/groupStyle">
                <ImageView android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_24px"
                    android:layout_marginEnd="@dimen/_25px"
                    android:layout_marginStart="@dimen/_25px" android:layout_width="@dimen/_24px"/>
                <TextView android:gravity="top|center"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_4px"
                    android:layout_width="match_parent"
                    android:text="@string/lbl_account" style="@style/textStylePoppinsregular10"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
