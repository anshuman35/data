<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="exploreRowModel" type="com.bhubaneswar.app.modules.explore.data.model.ExploreRowModel"/>
    </data>
    <LinearLayout android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle">
        <ImageView android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_70px" android:layout_width="@dimen/_70px"/>
        <TextView android:gravity="top|center"
            android:id="@+id/txtManShirt"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_8px"
            android:layout_width="match_parent"
            android:text="@{exploreRowModel.txtManShirt}" style="@style/textStylePoppinsregular10"/>
    </LinearLayout>
</layout>
