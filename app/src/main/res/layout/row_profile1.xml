<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="profile1RowModel" type="com.bhubaneswar.app.modules.profile.data.model.Profile1RowModel"/>
    </data>
    <LinearLayout android:clickable="true" android:id="@+id/linear"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_1">
        <LinearLayout android:layout_gravity="center_horizontal"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_15px"
            android:layout_marginEnd="@dimen/_16px"
            android:layout_marginStart="@dimen/_16px"
            android:layout_marginTop="@dimen/_15px"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_24px" android:layout_width="@dimen/_24px"/>
            <TextView android:gravity="top|start"
                android:id="@+id/txtGender"
                android:layout_gravity="center_vertical"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_16px"
                android:layout_weight="0.19" android:layout_width="0dp"
                android:text="@{profile1RowModel.txtGender}" style="@style/textStylePoppinsbold12_2"/>
            <TextView android:gravity="top|end"
                android:id="@+id/txtMale"
                android:layout_gravity="center_vertical"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_50px"
                android:layout_weight="0.38" android:layout_width="0dp"
                android:text="@{profile1RowModel.txtMale}" style="@style/textStylePoppinsregular12"/>
            <ImageView android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_24px"
                android:layout_marginStart="@dimen/_16px" android:layout_width="@dimen/_24px"/>
        </LinearLayout>
    </LinearLayout>
</layout>
