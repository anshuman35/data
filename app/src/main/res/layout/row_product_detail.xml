<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="productDetailRowModel" type="com.bhubaneswar.app.modules.productdetail.data.model.ProductDetailRowModel"/>
    </data>
    <LinearLayout android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
        <ImageView android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_109px"
            android:layout_marginEnd="@dimen/_16px"
            android:layout_marginStart="@dimen/_16px"
            android:layout_marginTop="@dimen/_16px"
            android:layout_width="@dimen/_109px"
            android:scaleType="fitCenter" cornerRadius="@{@dimen/_5px}" imageRes="@{@drawable/_59d05b3141b4f29f3753da2132aedf51e02e684a}"/>
        <TextView android:gravity="top|start"
            android:id="@+id/txtFsNikeAirM"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16px"
            android:layout_marginStart="@dimen/_16px"
            android:layout_marginTop="@dimen/_8px"
            android:layout_width="wrap_content"
            android:text="@{productDetailRowModel.txtFsNikeAirM}" style="@style/textStylePoppinsbold12_2"/>
        <TextView android:gravity="top|start" android:id="@+id/txt29943"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16px"
            android:layout_marginStart="@dimen/_16px"
            android:layout_marginTop="@dimen/_8px"
            android:layout_width="wrap_content"
            android:text="@{productDetailRowModel.txt29943}" style="@style/textStylePoppinsbold12"/>
        <LinearLayout android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16px"
            android:layout_marginStart="@dimen/_16px"
            android:layout_marginTop="@dimen/_8px"
            android:layout_width="wrap_content"
            android:orientation="horizontal" style="@style/groupStyle">
            <TextView android:gravity="top|start"
                android:id="@+id/txt53433"
                android:layout_gravity="center_vertical"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="@{productDetailRowModel.txt53433}" style="@style/textStylePoppinsregular10"/>
            <TextView android:gravity="top|start"
                android:id="@+id/txt24Off"
                android:layout_gravity="center_vertical"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_8px"
                android:layout_width="wrap_content"
                android:text="@{productDetailRowModel.txt24Off}" style="@style/textStylePoppinsbold10"/>
        </LinearLayout>
    </LinearLayout>
</layout>
