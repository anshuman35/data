<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="addAddressVM" type="com.bhubaneswar.app.modules.addaddress.data.viewmodel.AddAddressVM"/>
    </data>
    <LinearLayout android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" android:paddingEnd="@dimen/_16px"
        android:paddingStart="@dimen/_16px" style="@style/groupStylewhite_1">
        <androidx.appcompat.widget.Toolbar android:id="@+id/toolbar"
            android:layout_height="wrap_content" android:layout_width="match_parent">
            <LinearLayout android:layout_gravity="start"
                android:layout_height="@dimen/_24px"
                android:layout_marginEnd="@dimen/_50px"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24px" android:layout_width="@dimen/_24px"/>
                <TextView android:gravity="top|start"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_12px"
                    android:layout_weight="0.84"
                    android:layout_width="0dp"
                    android:text="@string/lbl_add_address" style="@style/textStylePoppinsbold16"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:layout_height="0dp"
            android:layout_marginTop="@dimen/_45px"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyle">
                    <TextView android:gravity="top|start"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_50px"
                        android:layout_width="match_parent"
                        android:text="@string/msg_country_or_regi" style="@style/textStylePoppinsbold14_2"/>
                    <EditText
                        android:background="@drawable/rectangle_border_blue_50_radius_5"
                        android:fontFamily="@font/poppins"
                        android:gravity="top|start"
                        android:hint="@string/msg_enter_the_count"
                        android:id="@+id/etEnterTheCount"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12px"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13px"
                        android:paddingStart="@dimen/_19px"
                        android:paddingTop="@dimen/_13px"
                        android:singleLine="true"
                        android:text="@={addAddressVM.addAddressModel.etEnterTheCountValue}"
                        android:textColor="@color/bluegray_300"
                        android:textColorHint="@color/bluegray_300"
                        android:textFontWeight="400"
                        android:textSize="@dimen/_12px" android:textStyle="normal"/>
                </LinearLayout>
                <LinearLayout android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_24px"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyle">
                    <TextView android:gravity="top|start"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_50px"
                        android:layout_width="match_parent"
                        android:text="@string/lbl_first_name" style="@style/textStylePoppinsbold14_2"/>
                    <EditText
                        android:background="@drawable/rectangle_border_blue_50_radius_5"
                        android:fontFamily="@font/poppins"
                        android:gravity="top|start"
                        android:hint="@string/msg_enter_the_first"
                        android:id="@+id/etEnterTheFirst"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12px"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13px"
                        android:paddingStart="@dimen/_19px"
                        android:paddingTop="@dimen/_13px"
                        android:singleLine="true"
                        android:text="@={addAddressVM.addAddressModel.etEnterTheFirstValue}"
                        android:textColor="@color/bluegray_300"
                        android:textColorHint="@color/bluegray_300"
                        android:textFontWeight="400"
                        android:textSize="@dimen/_12px" android:textStyle="normal"/>
                </LinearLayout>
                <LinearLayout android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_23px"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyle">
                    <TextView android:gravity="top|start"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_50px"
                        android:layout_width="match_parent"
                        android:text="@string/lbl_last_name" style="@style/textStylePoppinsbold14_2"/>
                    <EditText
                        android:background="@drawable/rectangle_border_blue_50_radius_5"
                        android:fontFamily="@font/poppins"
                        android:gravity="top|start"
                        android:hint="@string/msg_enter_the_last"
                        android:id="@+id/etEnterTheLast"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12px"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13px"
                        android:paddingStart="@dimen/_19px"
                        android:paddingTop="@dimen/_13px"
                        android:singleLine="true"
                        android:text="@={addAddressVM.addAddressModel.etEnterTheLastValue}"
                        android:textColor="@color/bluegray_300"
                        android:textColorHint="@color/bluegray_300"
                        android:textFontWeight="400"
                        android:textSize="@dimen/_12px" android:textStyle="normal"/>
                </LinearLayout>
                <LinearLayout android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_24px"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyle">
                    <TextView android:gravity="top|start"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_50px"
                        android:layout_width="match_parent"
                        android:text="@string/lbl_street_address" style="@style/textStylePoppinsbold14_2"/>
                    <EditText
                        android:background="@drawable/rectangle_border_blue_50_radius_5"
                        android:fontFamily="@font/poppins"
                        android:gravity="top|start"
                        android:hint="@string/msg_enter_the_stree"
                        android:id="@+id/etEnterTheStree"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12px"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13px"
                        android:paddingStart="@dimen/_19px"
                        android:paddingTop="@dimen/_13px"
                        android:singleLine="true"
                        android:text="@={addAddressVM.addAddressModel.etEnterTheStreeValue}"
                        android:textColor="@color/bluegray_300"
                        android:textColorHint="@color/bluegray_300"
                        android:textFontWeight="400"
                        android:textSize="@dimen/_12px" android:textStyle="normal"/>
                </LinearLayout>
                <LinearLayout android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_22px"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyle">
                    <TextView android:gravity="top|start"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_50px"
                        android:layout_width="match_parent"
                        android:text="@string/msg_street_address" style="@style/textStylePoppinsbold14_2"/>
                    <EditText
                        android:background="@drawable/rectangle_border_blue_50_radius_5"
                        android:fontFamily="@font/poppins"
                        android:gravity="top|start"
                        android:hint="@string/msg_enter_the_stree"
                        android:id="@+id/etEnterTheStree1"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_16px"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13px"
                        android:paddingStart="@dimen/_19px"
                        android:paddingTop="@dimen/_13px"
                        android:singleLine="true"
                        android:text="@={addAddressVM.addAddressModel.etEnterTheStree1Value}"
                        android:textColor="@color/bluegray_300"
                        android:textColorHint="@color/bluegray_300"
                        android:textFontWeight="400"
                        android:textSize="@dimen/_12px" android:textStyle="normal"/>
                </LinearLayout>
                <LinearLayout android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_22px"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyle">
                    <TextView android:gravity="top|start"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_50px"
                        android:layout_width="match_parent"
                        android:text="@string/lbl_city" style="@style/textStylePoppinsbold14_2"/>
                    <EditText
                        android:background="@drawable/rectangle_border_blue_50_radius_5"
                        android:fontFamily="@font/poppins"
                        android:gravity="top|start"
                        android:hint="@string/lbl_enter_the_city"
                        android:id="@+id/etEnterTheCity"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12px"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13px"
                        android:paddingStart="@dimen/_19px"
                        android:paddingTop="@dimen/_13px"
                        android:singleLine="true"
                        android:text="@={addAddressVM.addAddressModel.etEnterTheCityValue}"
                        android:textColor="@color/bluegray_300"
                        android:textColorHint="@color/bluegray_300"
                        android:textFontWeight="400"
                        android:textSize="@dimen/_12px" android:textStyle="normal"/>
                </LinearLayout>
                <LinearLayout android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_24px"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyle">
                    <TextView android:gravity="top|start"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_50px"
                        android:layout_width="match_parent"
                        android:text="@string/msg_state_province" style="@style/textStylePoppinsbold14_2"/>
                    <EditText
                        android:background="@drawable/rectangle_border_blue_50_radius_5"
                        android:fontFamily="@font/poppins"
                        android:gravity="top|start"
                        android:hint="@string/msg_enter_the_state"
                        android:id="@+id/etEnterTheState"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12px"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13px"
                        android:paddingStart="@dimen/_19px"
                        android:paddingTop="@dimen/_13px"
                        android:singleLine="true"
                        android:text="@={addAddressVM.addAddressModel.etEnterTheStateValue}"
                        android:textColor="@color/bluegray_300"
                        android:textColorHint="@color/bluegray_300"
                        android:textFontWeight="400"
                        android:textSize="@dimen/_12px" android:textStyle="normal"/>
                </LinearLayout>
                <LinearLayout android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_24px"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyle">
                    <TextView android:gravity="top|start"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_50px"
                        android:layout_width="match_parent"
                        android:text="@string/lbl_zip_code" style="@style/textStylePoppinsbold14_2"/>
                    <EditText
                        android:background="@drawable/rectangle_border_blue_50_radius_5"
                        android:fontFamily="@font/poppins"
                        android:gravity="top|start"
                        android:hint="@string/msg_enter_the_zip_c"
                        android:id="@+id/etEnterTheZipC"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12px"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13px"
                        android:paddingStart="@dimen/_19px"
                        android:paddingTop="@dimen/_13px"
                        android:singleLine="true"
                        android:text="@={addAddressVM.addAddressModel.etEnterTheZipCValue}"
                        android:textColor="@color/bluegray_300"
                        android:textColorHint="@color/bluegray_300"
                        android:textFontWeight="400"
                        android:textSize="@dimen/_12px" android:textStyle="normal"/>
                </LinearLayout>
                <LinearLayout android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_24px"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyle">
                    <TextView android:gravity="top|start"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_50px"
                        android:layout_width="match_parent"
                        android:text="@string/lbl_phone_number" style="@style/textStylePoppinsbold14_2"/>
                    <EditText
                        android:background="@drawable/rectangle_border_blue_50_radius_5"
                        android:fontFamily="@font/poppins"
                        android:gravity="top|start"
                        android:hint="@string/msg_enter_the_phone"
                        android:id="@+id/etEnterThePhone"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12px"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13px"
                        android:paddingStart="@dimen/_19px"
                        android:paddingTop="@dimen/_13px"
                        android:singleLine="true"
                        android:text="@={addAddressVM.addAddressModel.etEnterThePhoneValue}"
                        android:textColor="@color/bluegray_300"
                        android:textColorHint="@color/bluegray_300"
                        android:textFontWeight="400"
                        android:textSize="@dimen/_12px" android:textStyle="normal"/>
                </LinearLayout>
                <TextView android:gravity="top|center"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_20px"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_16px"
                    android:paddingEnd="@dimen/_30px"
                    android:paddingStart="@dimen/_30px"
                    android:paddingTop="@dimen/_16px"
                    android:text="@string/lbl_add_address" style="@style/textStylePoppinsbold14"/>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
