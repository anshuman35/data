<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="notificationActivityActivityList01Model" type="com.bhubaneswar.app.modules.notificationactivityactivitylist01.data.model.NotificationActivityActivityList01Model"/>
    </data>
    <LinearLayout android:layout_gravity="center_horizontal"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStylewhite_1">
        <ImageView android:layout_gravity="top"
            android:layout_height="@dimen/_24px"
            android:layout_marginStart="@dimen/_16px" android:layout_width="@dimen/_24px"/>
        <LinearLayout android:layout_gravity="center_vertical"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_12px"
            android:layout_weight="0.85" android:layout_width="0dp"
            android:orientation="vertical" style="@style/groupStyle">
            <TextView android:gravity="top|start"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:text="@string/msg_transaction_nik" style="@style/textStylePoppinsbold14_2"/>
            <TextView android:gravity="top|start"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8px"
                android:layout_width="match_parent"
                android:text="@string/msg_culpa_cillum_co" style="@style/textStylePoppinsregular12"/>
            <TextView android:gravity="top|start"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8px"
                android:layout_width="match_parent"
                android:text="@string/msg_april_30_2014" style="@style/textStylePoppinsregular10_1"/>
        </LinearLayout>
    </LinearLayout>
</layout>
